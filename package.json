{
  "name": "my-retail-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack --optimize-minimize --color && npm run build:lib",
    "build-watch": "npm run build -- -- --watch",
    "build:lib": "babel src --out-dir lib",
    "clean": "rm -rf dist",
    "lint": "eslint javascript --ext js --ext jsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:check": "git ls-tree -r HEAD --name-only | grep \\.js$ | xargs node ./node_modules/eslint/bin/eslint.js",
    "prepublish": "npm run build",
    "start": "webpack-dev-server --open --content-base src/static/",
    "test": "NODE_ENV=libprice-build jest --coverage"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "keywords": [
    "my-retail"
  ],
  "author": "Meenu Narula",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "bootstrap": "^3.3.7",
    "bootstrap-loader": "^2.2.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^3.8.2",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-loader": "^2.0.9",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.1.3",
    "react-responsive-carousel": "^3.1.30",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "react": "^16.2.0"
  }
}
